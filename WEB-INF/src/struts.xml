			<result name="update">/poscenter/event/updateMoneyDiscountEvent.jsp</result>
			<result name="detail">/poscenter/event/showMoneyDiscountEvent.jsp</result>
		</action>
		<action name="priceCard" class="mmb.poscenter.action.PriceCardAction">
			<result name="success">/poscenter/pricecard/priceCardList.jsp</result>
			<result name="input">/poscenter/pricecard/priceCardForm.jsp</result>
			<result name="selectPriceCardList">/poscenter/pricecard/selectPriceCardList.jsp</result>
			<result name="open">/poscenter/pricecard/openCardForm.jsp</result>
			<result name="update">/poscenter/pricecard/updateCardForm.jsp</result>
			<result name="detail">/poscenter/pricecard/priceCardDetail.jsp</result>
		</action>
		
