<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

  <constant name="struts.devMode" value="true" />
  <constant name="struts.action.extension" value="do" />
  <constant name="struts.convention.result.path" value="/"/>
  <constant name="struts.convention.package.locators" value="mmboa"/>
  <constant name="struts.convention.default.parent.package" value="struts-default"/> 
  <constant name="struts.multipart.maxSize" value="20000000" />

  <package name="struts-default2" abstract="true" extends="struts-default">
        <result-types>
            <result-type name="chain" class="com.opensymphony.xwork2.ActionChainResult"/>
            <result-type name="dispatcher" class="org.apache.struts2.dispatcher.ServletDispatcherResult" default="true"/>
            <result-type name="freemarker" class="org.apache.struts2.views.freemarker.FreemarkerResult"/>
            <result-type name="httpheader" class="org.apache.struts2.dispatcher.HttpHeaderResult"/>
            <result-type name="redirect" class="org.apache.struts2.dispatcher.ServletRedirectResult"/>
            <result-type name="redirectAction" class="org.apache.struts2.dispatcher.ServletActionRedirectResult"/>
            <result-type name="stream" class="org.apache.struts2.dispatcher.StreamResult"/>
            <result-type name="velocity" class="org.apache.struts2.dispatcher.VelocityResult"/>
            <result-type name="xslt" class="org.apache.struts2.views.xslt.XSLTResult"/>
            <result-type name="plainText" class="org.apache.struts2.dispatcher.PlainTextResult" />
        </result-types>
        <interceptors>
        	<interceptor name="fileUpload" class="org.apache.struts2.interceptor.FileUploadInterceptor"/>
        </interceptors>
        <default-class-ref class="com.opensymphony.xwork2.ActionSupport" />
  </package>

	<package name="ss" namespace="/pos" extends="struts-default2">
		<global-results>
			<result name="error">/error.jsp</result>
		</global-results>
		
		<action name="Product" class="mmb.poscenter.action.ProductAction">
			<result name="success">/poscenter/product/ProductList.jsp</result>
			<result name="input">/poscenter/product/productForm.jsp</result>
			<result name="detail">/poscenter/product/productDetail.jsp</result>
			<result name="showBarcode">/poscenter/product/showBarcode.jsp</result>
			<result name="selectProductList">/poscenter/product/selectProductList.jsp</result>
		</action>
		
		<action name="goodsClass" class="mmb.poscenter.action.GoodsClassAction">
			<result name="success">/poscenter/goodsclass/goodsClassList.jsp</result>
			<result name="input">/poscenter/goodsclass/goodsClassForm.jsp</result>
			<result name="detail">/poscenter/goodsclass/goodsClassDetail.jsp</result>
		</action>
		
	    <action name="supplier" class="mmb.poscenter.action.SupplierAction">
			<result name="success">/poscenter/supplier/supplierList.jsp</result>
			<result name="input">/poscenter/supplier/supplierForm.jsp</result>
			<result name="detail">/poscenter/supplier/supplierDetail.jsp</result>
			<result name="choose">/poscenter/supplier/supplierChooseList.jsp</result>
		</action>
		
		<action name="purchaseOrder" class="mmb.poscenter.action.PurchaseOrderAction">
			<result name="success">/poscenter/purchaseorder/purchaseOrderList.jsp</result>
			<result name="input">/poscenter/purchaseorder/purchaseOrderForm.jsp</result>
		</action>
		<action name="purchaseOrderItem" class="mmb.poscenter.action.PurchaseOrderItemAction">
			<result name="success">/poscenter/purchaseorder/purchaseOrderItemList.jsp</result>
			<result name="input">/poscenter/purchaseorder/purchaseOrderItemForm.jsp</result>
		</action>
		
		<action name="sendOrder" class="mmb.poscenter.action.SendOrderAction">
			<result name="success">/poscenter/sendorder/sendOrderList.jsp</result>
			<result name="input">/poscenter/sendorder/sendOrderForm.jsp</result>
		</action>
		<action name="sendOrderItem" class="mmb.poscenter.action.SendOrderItemAction">
			<result name="success">/poscenter/sendorder/sendOrderItemList.jsp</result>
			<result name="input">/poscenter/sendorder/sendOrderItemForm.jsp</result>
		</action>
		
		<action name="receiveOrder" class="mmb.poscenter.action.ReceiveOrderAction">
			<result name="success">/poscenter/receiveorder/receiveorderList.jsp</result>
			<result name="acquire">/poscenter/receiveorder/receiveorderform.jsp</result>
		</action>
		
		<action name="receiveOrderItem" class="mmb.poscenter.action.ReceiveOrderItemAction">
			<result name="success">/poscenter/receiveorder/receiveorderItemList.jsp</result>
		</action>
		
		<action name="invoice" class="mmb.poscenter.action.InvoiceAction">
			<result name="productList">/poscenter/invoice/productList.jsp</result>
			<result name="success">/poscenter/invoice/invoiceList.jsp</result>
		</action>
		
		<action name="shop" class="mmb.poscenter.action.ShopAction">
			<result name="success">/poscenter/shop/shopList.jsp</result>
			<result name="input">/poscenter/shop/shopForm.jsp</result>
			<result name="detail">/poscenter/shop/shopDetail.jsp</result>
