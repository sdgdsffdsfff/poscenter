			<result name="auditList">/poscenter/rechargeorder/auditRechargeOrderList.jsp</result>
			<result name="input">/poscenter/rechargeorder/rechargeOrderForm.jsp</result>
		</action>
		<action name="adjustPriceBill" class="mmb.poscenter.action.AdjustPriceBillAction">
			<result name="success">/poscenter/adjustpricebill/adjustPriceBillList.jsp</result>
		</action>
		
	</package>

	<package name="pos-interface" namespace="/enter" extends="struts-default2">
		<action name="posadmin" class="mmb.poscenter.action.PosadminAction">
		</action>
	</package>
</struts>