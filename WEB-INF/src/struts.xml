			<result name="input">/poscenter/auth/company/companyForm.jsp</result>
			<result name="selectCompanyList">/poscenter/auth/company/selectCompanyList.jsp</result>
		</action>
		<action name="department" class="mmb.auth.action.DepartmentAction">
			<result name="success">/poscenter/auth/department/departmentList.jsp</result>
			<result name="input">/poscenter/auth/department/departmentForm.jsp</result>
			<result name="choose">/poscenter/auth/department/departmentChooseList.jsp</result>
		</action>
		<action name="resource" class="mmb.auth.action.ResourceAction">
			<result name="success">/poscenter/auth/resource/resourceList.jsp</result>
			<result name="input">/poscenter/auth/resource/resourceForm.jsp</result>
			<result name="detail">/poscenter/auth/resource/resourceDetail.jsp</result>
		</action>
		<action name="menu" class="mmb.auth.action.MenuAction">
			<result name="success">/poscenter/auth/menu/menuList.jsp</result>
			<result name="input">/poscenter/auth/menu/menuForm.jsp</result>
			<result name="detail">/poscenter/auth/menu/menuDetail.jsp</result>
			<result name="secondLevelMenuList">/poscenter/desktop/left.jsp</result>
		</action>
		<action name="user" class="mmb.auth.action.UserAction">
			<result name="success">/poscenter/auth/user/userList.jsp</result>
			<result name="input">/poscenter/auth/user/userForm.jsp</result>
			<result name="detail">/poscenter/auth/user/userDetail.jsp</result>
		</action>
