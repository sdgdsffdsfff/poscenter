		</action>
		
		<action name="member" class="mmb.poscenter.action.MemberAction">
			<result name="success">/poscenter/member/memberList.jsp</result>
			<result name="detail">/poscenter/member/memberDetail.jsp</result>
		</action>
		
		<action name="checkStock" class="mmb.poscenter.action.CheckStockAction">
			<result name="success">/poscenter/checkstock/checkStockList.jsp</result>
		</action>
		<action name="checkStockItem" class="mmb.poscenter.action.CheckStockItemAction">
			<result name="success">/poscenter/checkstock/checkStockItemList.jsp</result>
			<result name="printCheckStockView">/poscenter/checkstock/printCheckStock.jsp</result>
		</action>
		
		<action name="shopStock" class="mmb.poscenter.action.ShopStockAction">
			<result name="productList">/poscenter/shopstock/productList.jsp</result>
			<result name="success">/poscenter/shopstock/shopStockList.jsp</result>
		</action>
		
		<action name="shopReturnOrder" class="mmb.poscenter.action.ShopReturnOrderAction">
			<result name="success">/poscenter/shopreturnorder/shopReturnOrderList.jsp</result>
		</action>
		<action name="shopReturnOrderItem" class="mmb.poscenter.action.ShopReturnOrderItemAction">
			<result name="success">/poscenter/shopreturnorder/shopReturnOrderItemList.jsp</result>
		</action>
		<action name="shopOrder" class="mmb.poscenter.action.ShopOrderAction">
			<result name="shopLeaseOrderList">/poscenter/shoporder/leaseOrderList.jsp</result>
			<result name="shopSaledOrderList">/poscenter/shoporder/saledOrderList.jsp</result>
		</action>
		
		<action name="shopSaledOrderEvent" class="mmb.poscenter.action.ShopSaledOrderEventAction">
			<result name="success">/poscenter/shoporder/showSaledOrderEvent.jsp</result>
		</action>
		
		<action name="shopInvoice" class="mmb.poscenter.action.ShopInvoiceAction">
			<result name="productList">/poscenter/shopinvoice/productList.jsp</result>
			<result name="success">/poscenter/shopinvoice/shopInvoiceList.jsp</result>
		</action>
		
		<action name="event" class="mmb.poscenter.action.EventAction">
			<result name="success">/poscenter/event/eventList.jsp</result>
		</action>
		<action name="buyGiftEvent" class="mmb.poscenter.action.BuyGiftEventAction">
			<result name="success">/poscenter/event/eventList.jsp</result>
			<result name="add">/poscenter/event/addBuyGiftEvent.jsp</result>
			<result name="update">/poscenter/event/updateBuyGiftEvent.jsp</result>
			<result name="detail">/poscenter/event/showBuyGiftEvent.jsp</result>
		</action>
		<action name="swapBuyEvent" class="mmb.poscenter.action.SwapBuyEventAction">
			<result name="input">/poscenter/event/swapBuyEventForm.jsp</result>
			<result name="update">/poscenter/event/updateSwapBuyEventForm.jsp</result>
